body { 
		background-color: #E9ECEF; 
		margin: 0; 
		padding: 0; 
		font-family:Garamond, "Times New Roman", serif; 
		font-size: 10pt;
		vertical-align: top;
}

a,a:visited,a:active { 
	color: #225566; 
	font-weight: bold; 
	text-decoration: none; 
}
a:hover { 
	color: #226822; 
	background-color: #f5f5b6; 
	text-decoration: none; 
}

#main	{ background-color:#FFFFFF; height:100%; margin:0; }
#mid	{ height:auto; }
#header { background-color:#C7D0D9; border-bottom:0px solid #8CACBB; height:41px; width:96%; margin-left:2%; }
#footer { background-color:#C7D0D9; border-top:0px solid #8CACBB; height:41px; width:96%; margin-left:2%; padding-top:20%; }

#left	{ background-color:#F3F6F9; border:0px solid #8CACBB; height:100%; width:20%; margin-top:4px; margin-left:2%; margin-right:1%; float:left; }
#center { background-color:#F3F6F9; border:0px solid #8CACBB; height:100%; width:54%; margin-top:4px; margin-right:1%; float:left;} 
#right	{ background-color:#F3F6F9; border:0px solid #8CACBB; height:100%; width:20%; margin-top:4px; float:left; } 

table.buttons	{ border:1px solid #8CACBB; width: 94%; margin-top:2%; margin-left:3%; margin-right:3%; }
td.buttons		{ background-color:#E3E6B9; border:1px solid #8CACBB; text-align:center; }

a.button,a:visited.button,a:active.button { font-size: 12px; color : #223344; padding: 1px 2px 1px 2px; border: 0px solid #000000; }
a:hover.button { font-size:12px; color:#556677; padding:1px 2px 1px 2px; border: 0px solid #000000; text-decoration: underline; }


table.forms { height:98%; width: 98%; border:0px; margin:1%; font-family:Garamond, "Times New Roman", serif; font-size:10pt; }
input.forms { border:1px solid #8CACBB; font-family:Garamond, "Times New Roman", serif; font-size:10pt; color:#223344;}
td.forml	{ height:98%; width: 20%; border:1px; background-color:#E3E6B9; border:1px solid #8CACBB; text-align:right; font-weight:bold; white-space:nowrap;}
td.formr	{ height:98%; width: 78%; background-color:#F3F6F9; }
td.formerr  { width: 98%; background-color:#FF6644; }
td.formdone { width: 98%; background-color:#2288AA; }

div.centered {}
